{
  "name": "middleware",
  "version": "0.1.5",
  "description": "The backend for the recruiters.rip frontend",
  "author": "Obafemi Teminife",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:ssl": "yarn build && NODE_ENV=production yarn start:prod",
    "lint": "eslint \"{src,libs}/**/*.ts\" --fix --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@sentry/node": "^7.51.2",
    "@sentry/tracing": "^7.51.2",
    "@twirelab/nestjs-auth0": "^0.2.0",
    "auth0": "^3.4.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "ethers": "6.3.0",
    "express-basic-auth": "^1.2.1",
    "express-jwt": "^8.4.1",
    "express-session": "^1.17.3",
    "fast-sort": "^3.4.0",
    "fp-ts": "^2.15.0",
    "helmet": "^7.0.0",
    "io-ts": "^2.2.20",
    "io-ts-human-reporter": "^0.1.2",
    "iron-session": "^6.3.1",
    "joi": "^17.9.2",
    "mime": "^3.0.0",
    "neo4j-driver": "^5.8.0",
    "neogma": "^1.12.2",
    "nest-neo4j": "^0.3.1",
    "nest-neogma": "^0.4.2",
    "nft.storage": "^7.0.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "siwe": "^2.1.4",
    "ts-morph": "^18.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.7",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/auth0": "^3.3.2",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "20.1.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "node-notifier": "^10.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "semantic-release": "^21.0.2",
    "semantic-release-slack-bot": "^4.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ]
  }
}
